<Application
    x:Class="Sudoku.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Sudoku"
    StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="GameButtonsColor">#DDFFDD</SolidColorBrush>
        <SolidColorBrush x:Key="ForgroundColor">#004411</SolidColorBrush>
        <SolidColorBrush x:Key="BadForgroundColor">#FF1111</SolidColorBrush>
        <SolidColorBrush x:Key="SelectedColor">#BBFFCE</SolidColorBrush>
        <SolidColorBrush x:Key="SelectedCellColor">#77FF88</SolidColorBrush>
    </Application.Resources>
</Application>
